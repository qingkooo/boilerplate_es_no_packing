{
  "name": "boilerplate_es_no_packing",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "packages/*",
        "apps/*"
      ]
    },
    "apps/client": {
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "pkg1": "file:packages/pkg1"
      }
    },
    "apps/client/node_modules/pkg1": {
      "resolved": "apps/client/packages/pkg1",
      "link": true
    },
    "apps/client/packages/pkg1": {},
    "node_modules/client": {
      "resolved": "apps/client",
      "link": true
    },
    "node_modules/pkg1": {
      "resolved": "packages/pkg1",
      "link": true
    },
    "node_modules/pkg2": {
      "resolved": "packages/pkg2",
      "link": true
    },
    "packages/pkg1": {
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "pkg2": "file:packages/pkg2"
      }
    },
    "packages/pkg1/node_modules/pkg2": {
      "resolved": "packages/pkg1/packages/pkg2",
      "link": true
    },
    "packages/pkg1/packages/pkg2": {},
    "packages/pkg2": {
      "version": "1.0.0",
      "license": "MIT"
    }
  },
  "dependencies": {
    "client": {
      "version": "file:apps/client",
      "requires": {
        "pkg1": "file:packages/pkg1"
      },
      "dependencies": {
        "pkg1": {
          "version": "file:apps/client/packages/pkg1"
        }
      }
    },
    "pkg1": {
      "version": "file:packages/pkg1",
      "requires": {
        "pkg2": "file:packages/pkg2"
      },
      "dependencies": {
        "pkg2": {
          "version": "file:packages/pkg1/packages/pkg2"
        }
      }
    },
    "pkg2": {
      "version": "file:packages/pkg2"
    }
  }
}
